<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/trans_prog_percent1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/trans_prog_bar1"
        android:layout_marginRight="29dp"
        android:text="percent1" />

    <ProgressBar
        android:id="@+id/trans_prog_bar1"
        style="@android:style/Widget.ProgressBar.Horizontal"
	    android:max="100"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_name1"
        android:layout_below="@+id/trans_prog_title"
        android:layout_marginTop="35dp" />

    <TextView
        android:id="@+id/trans_prog_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="titile" />

    <TextView
        android:id="@+id/trans_prog_name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/trans_prog_percent1"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="38dp"
        android:text="name2" />

    <TextView
        android:id="@+id/trans_prog_name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/trans_prog_percent1"
        android:layout_alignLeft="@+id/trans_prog_name2"
        android:text="name1" />

    <ProgressBar
        android:id="@+id/trans_prog_bar2"
        style="@android:style/Widget.ProgressBar.Horizontal"
	    android:max="100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_name2"
        android:layout_alignRight="@+id/trans_prog_bar1"
        android:layout_below="@+id/trans_prog_name2" />

    <TextView
        android:id="@+id/trans_prog_percent2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/trans_prog_bar2"
        android:layout_alignLeft="@+id/trans_prog_percent1"
        android:text="percent2" />

    <TextView
        android:id="@+id/trans_prog_name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_bar2"
        android:layout_below="@+id/trans_prog_bar2"
        android:layout_marginTop="38dp"
        android:text="name3" />

    <ProgressBar
        android:id="@+id/trans_prog_bar3"
        style="@android:style/Widget.ProgressBar.Horizontal"
	    android:max="100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_name3"
        android:layout_alignRight="@+id/trans_prog_bar2"
        android:layout_below="@+id/trans_prog_name3" />

    <TextView
        android:id="@+id/trans_prog_percent3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/trans_prog_bar3"
        android:layout_alignRight="@+id/trans_prog_percent2"
        android:text="percent3" />

    <TextView
        android:id="@+id/trans_prog_name4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_bar3"
        android:layout_below="@+id/trans_prog_bar3"
        android:layout_marginTop="38dp"
        android:text="name4" />

    <ProgressBar
        android:id="@+id/trans_prog_bar4"
        style="@android:style/Widget.ProgressBar.Horizontal"
	    android:max="100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_name4"
        android:layout_alignRight="@+id/trans_prog_bar3"
        android:layout_below="@+id/trans_prog_name4" />

    <TextView
        android:id="@+id/trans_prog_percent4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_percent3"
        android:layout_alignTop="@+id/trans_prog_bar4"
        android:text="percent4" />

    <TextView
        android:id="@+id/trans_prog_name5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_bar4"
        android:layout_below="@+id/trans_prog_percent4"
        android:layout_marginTop="38dp"
        android:text="name5" />

    <ProgressBar
        android:id="@+id/trans_prog_bar5"
        style="@android:style/Widget.ProgressBar.Horizontal"
	    android:max="100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/trans_prog_name5"
        android:layout_alignRight="@+id/trans_prog_bar4"
        android:layout_below="@+id/trans_prog_name5" />

    <TextView
        android:id="@+id/trans_prog_percent5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/trans_prog_bar5"
        android:layout_alignRight="@+id/trans_prog_percent4"
        android:text="percent5" />

</RelativeLayout>
